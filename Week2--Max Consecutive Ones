class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int res=0;
        int max=0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==0)
                res=0;
            else{
                res++;
                max=Math.max(res,max);
                
            }
            
        }
        return max;
    }
}
